<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.healthcare.home.controllers.BedDetailsController"
      spacing="10" alignment="CENTER_LEFT" style="-fx-padding:15;">
    <Label fx:id="bedIdLabel" style="-fx-font-size:16;"/>
    <Label fx:id="residentLabel" />
    <TextArea fx:id="details" prefRowCount="10" prefColumnCount="30" editable="false"/>
</VBox>
